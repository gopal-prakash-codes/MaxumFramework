   "manifest": {
      "host_permissions": [
         "http://*/*"
      ],
      "key": "$CRX_PUBLIC_KEY",
      "oauth2": {
         "client_id": "$OAUTH_CLIENT_ID",
         "scopes": [
            "https://www.googleapis.com/auth/userinfo.email",
            "https://www.googleapis.com/auth/userinfo.profile"
         ]
      },
      "permissions": [
         "contextMenus",
         "clipboard",
         "sidePanel",
         "identity",
